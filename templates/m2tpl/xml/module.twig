<?xml version="1.0"?>
{% if license %}
<!--
{{license}}
-->
{% endif %}
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Module/etc/module.xsd">
    <module name="{{vendor}}_{{module}}">
{% if depends | length > 0 %}
        <sequence>
{% for item in depends %}
            <module name="{{item}}"/>
{% endfor %}
        </sequence>
{% endif %}
    </module>
</config>
